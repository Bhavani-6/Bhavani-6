n=int(input("Enter the value of n:"))
lst=[]
count=0
for i in range(2,n+1):
    flag=0
    for j in range(2,i):
        if i%j==0:
            flag=1
    if flag==0:
        lst.append(i)
        count+=1
print("No of prime numbers",count)
print("prime numbers",*lst)
